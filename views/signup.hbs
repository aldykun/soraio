    <div class="row">
      <div class="col-lg-4 col-lg-push-4 col-sm-6 col-sm-push-3">
        <section class="content-inner">
          <div class="card">
            <div class="card-main">
              <div class="card-header">
                <div class="card-inner">
                  <h1 class="card-heading">Create an Account</h1>
                </div>
              </div>
              <div class="card-inner">
                <form class="form" action="/auth/signup" method="post">
                  <div class="row">
                    <div class="form-group form-group-label">
                        <div class="col-md-5 col-md-push-1">
                          <label class="floating-label" for="ui_signup_firstname">First Name</label>
                          <input class="form-control" id="ui_signup_firstname" type="text" name="firstName">
                        </div>
                    </div>
                    <div class="form-group form-group-label">
                        <div class="col-md-5 col-md-push-1">
                          <label class="floating-label" for="ui_signup_lastname">Last Name</label>
                          <input class="form-control" id="ui_signup_lastname" type="text" name="lastName">
                        </div>
                    </div>
                  </div>
                  <div class="form-group form-group-label">
                    <div class="row">
                      <div class="col-md-10 col-md-push-1">
                        <label class="floating-label" for="ui_signup_email">Input your email address</label>
                        <input class="form-control" id="ui_signup_email" type="email" name="email">
                      </div>
                    </div>
                  </div>
                  <div class="form-group form-group-label">
                    <div class="row">
                      <div class="col-md-10 col-md-push-1">
                        <label class="floating-label" for="ui_signup_username">Create your username</label>
                        <input class="form-control" id="ui_signup_username" type="text" name="username">
                      </div>
                    </div>
                  </div>
                  <div class="form-group form-group-label">
                    <div class="row">
                      <div class="col-md-10 col-md-push-1">
                        <label class="floating-label" for="ui_signup_password">Create a password</label>
                        <input class="form-control" id="ui_signup_password" type="password" name="password">
                      </div>
                    </div>
                  </div>
                  <div class="form-group form-group-label">
                    <div class="row">
                      <div class="col-md-10 col-md-push-1">
                        <label class="floating-label" for="ui_signup_copassword">Confirm password</label>
                        <input class="form-control" id="ui_signup_copassword" type="password" name="copassword">
                      </div>
                    </div>
                  </div>
                  <div class="form-group form-group-label">
                    <div class="row">
                      <div class="col-md-10 col-md-push-1">
                        <label class="floating-label" for="ui_signup_birth">Birthday</label>
                        <input class="form-control" id="ui_signup_birth" type="text" name="birth">
                      </div>
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="row">
                      <div class="col-md-10 col-md-push-1">
                        <button class="btn btn-block btn-sora waves-attach waves-effect" type="submit">Sign Up</button>
                      </div>
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="row">
                      <div class="col-md-10 col-md-push-1">
                        <hr/>
                        <div class="checkbox-adv text-center">
                          <label for="ui_signup_agree">
                            By clicking "Sign Up", you agree to our terms of service and privacy policy. We'll occasionally send you account related emails.
                          </label>
                        </div>
                        <hr/>
                        <div class="checkbox-adv">
                          <div class="row">
                            <label class="col-md-8">
                              Already have an account?
                            </label>
                            <label class="col-md-4 text-right">
                              <a href="/auth/login">Sign In</a>
                            </label>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <input type="hidden" name="_csrf" value="{{csrfToken}}">
                </form>
              </div>
            </div>
          </div>
          <div class="clearfix">
            <p class="margin-no-top text-center"><a class="btn btn-flat btn-sora waves-attach" href="javascript:void(0)">Learn more about why we ask for this information.</a></p>
          </div>
        </section>
      </div>
    </div>
